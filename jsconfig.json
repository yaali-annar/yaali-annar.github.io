{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "commonjs",
    "target": "es2018",
    "paths": {
      "@constants/*": ["./src/constants/*"],
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@styles/*": ["./src/styles/*"],
      "@routes/*": ["./src/routes/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@utils/*": ["./src/utils/*"],
    }
  },
  "exclude": ["node_modules"]
}
